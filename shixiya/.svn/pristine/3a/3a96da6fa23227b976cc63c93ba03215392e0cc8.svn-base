package com.dayee.wintalent.service.v8.pojo;

import javax.xml.bind.annotation.XmlElement;

public class CareerObjectives {

	private String DesiredTypeOfEmployment;//期望工作性质
	private String DesiredLocation;//期望工作地点
	private String DesiredPosition;//期望职能
	private String DesiredIndustry;//期望行业
	private String ExpectedAnnualSalary;//期望年薪
	private String ExpectedSalaryBeforeTax;//期望薪酬
	private String CurrentSalary;//目前薪酬
	private String OnBoardTime;//到岗时间
	private String CurrentIndustry;//当前行业
	private String CurrentFunction;//当前职能
	
	
	
	
	public CareerObjectives() {
		super();
	}

	public CareerObjectives(String desiredTypeOfEmployment,
			String desiredLocation, String desiredPosition,
			String desiredIndustry, String expectedAnnualSalary,
			String expectedSalaryBeforeTax, String currentSalary,
			String onBoardTime, String currentIndustry, String currentFunction) {
		super();
		DesiredTypeOfEmployment = desiredTypeOfEmployment;
		DesiredLocation = desiredLocation;
		DesiredPosition = desiredPosition;
		DesiredIndustry = desiredIndustry;
		ExpectedAnnualSalary = expectedAnnualSalary;
		ExpectedSalaryBeforeTax = expectedSalaryBeforeTax;
		CurrentSalary = currentSalary;
		OnBoardTime = onBoardTime;
		CurrentIndustry = currentIndustry;
		CurrentFunction = currentFunction;
	}
	@XmlElement(name="DesiredTypeOfEmployment")
	public String getDesiredTypeOfEmployment() {
		return DesiredTypeOfEmployment;
	}

	public void setDesiredTypeOfEmployment(String desiredTypeOfEmployment) {
		DesiredTypeOfEmployment = desiredTypeOfEmployment;
	}
	@XmlElement(name="DesiredLocation")
	public String getDesiredLocation() {
		return DesiredLocation;
	}

	public void setDesiredLocation(String desiredLocation) {
		DesiredLocation = desiredLocation;
	}
	@XmlElement(name="DesiredPosition")
	public String getDesiredPosition() {
		return DesiredPosition;
	}

	public void setDesiredPosition(String desiredPosition) {
		DesiredPosition = desiredPosition;
	}
	@XmlElement(name="DesiredIndustry")
	public String getDesiredIndustry() {
		return DesiredIndustry;
	}

	public void setDesiredIndustry(String desiredIndustry) {
		DesiredIndustry = desiredIndustry;
	}
	@XmlElement(name="ExpectedAnnualSalary")
	public String getExpectedAnnualSalary() {
		return ExpectedAnnualSalary;
	}

	public void setExpectedAnnualSalary(String expectedAnnualSalary) {
		ExpectedAnnualSalary = expectedAnnualSalary;
	}
	@XmlElement(name="ExpectedSalaryBeforeTax")
	public String getExpectedSalaryBeforeTax() {
		return ExpectedSalaryBeforeTax;
	}

	public void setExpectedSalaryBeforeTax(String expectedSalaryBeforeTax) {
		ExpectedSalaryBeforeTax = expectedSalaryBeforeTax;
	}
	@XmlElement(name="CurrentSalary")
	public String getCurrentSalary() {
		return CurrentSalary;
	}

	public void setCurrentSalary(String currentSalary) {
		CurrentSalary = currentSalary;
	}
	@XmlElement(name="OnBoardTime")
	public String getOnBoardTime() {
		return OnBoardTime;
	}

	public void setOnBoardTime(String onBoardTime) {
		OnBoardTime = onBoardTime;
	}
	@XmlElement(name="CurrentIndustry")
	public String getCurrentIndustry() {
		return CurrentIndustry;
	}

	public void setCurrentIndustry(String currentIndustry) {
		CurrentIndustry = currentIndustry;
	}
	@XmlElement(name="CurrentFunction")
	public String getCurrentFunction() {
		return CurrentFunction;
	}

	public void setCurrentFunction(String currentFunction) {
		CurrentFunction = currentFunction;
	}

	@Override
	public String toString() {
		return "CareerObjectives [DesiredTypeOfEmployment="
				+ DesiredTypeOfEmployment + ", DesiredLocation="
				+ DesiredLocation + ", DesiredPosition=" + DesiredPosition
				+ ", DesiredIndustry=" + DesiredIndustry
				+ ", ExpectedAnnualSalary=" + ExpectedAnnualSalary
				+ ", ExpectedSalaryBeforeTax=" + ExpectedSalaryBeforeTax
				+ ", CurrentSalary=" + CurrentSalary + ", OnBoardTime="
				+ OnBoardTime + ", CurrentIndustry=" + CurrentIndustry
				+ ", CurrentFunction=" + CurrentFunction + "]";
	}
	
	
	
	
	
}
